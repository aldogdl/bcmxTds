<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">
        <title>{% block title %}Buscomex{% endblock %}</title>
        <link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css">
        {{ encore_entry_link_tags('app') }}
        {% block stylesheets %}{% endblock %}
    </head>
    <body id="getEnv" data-env="{{ env }}">
        {% block body %}{% endblock %}

        <span id="login" data-base="{{ path('security') }}"></span>
        {{ encore_entry_script_tags('jqblock') }}
        {{ encore_entry_script_tags('app') }}
        {{ encore_entry_script_tags('http') }}
        <script id="cargando" type="text/x-jsrender">{% include 'renders/pl_cargando.html.twig' %}</script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
